    HostName 18.139.21.123
    User ubuntu
    IdentityFile ~/.ssh/LightsailDefaultKey-ap-southeast-1.pem
    ForwardAgent yes
    ProxyCommand nc -v -x 127.0.0.1:7890 %h %p

Host sg1-direct
    HostName 18.139.21.123
    User ubuntu
    IdentityFile ~/.ssh/LightsailDefaultKey-ap-southeast-1.pem
    ForwardAgent yes

