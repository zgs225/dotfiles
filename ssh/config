    User alwyzon
    ForwardAgent yes
    ProxyCommand nc -v -x 127.0.0.1:7890 %h %p

Host vi1-direct
    HostName vi1.yuez.me
    User alwyzon
    ForwardAgent yes

Host 172.20.10.3
    User root
    PubkeyAcceptedAlgorithms +ssh-rsa
    HostkeyAlgorithms +ssh-rsa

Host router
    User root
    HostName 192.168.2.1

Host home
    User root
    HostName home.yuez.me
    ForwardAgent yes

Host *
    StrictHostKeyChecking no
    ServerAliveInterval 10

Host github.com
    Hostname github.com
    ProxyCommand nc -v -x 127.0.0.1:7890 %h %p
    ServerAliveInterval 55
    ForwardAgent yes
